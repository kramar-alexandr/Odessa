procedure  ArrayFromArray(array string MotherArray, var array string newArray,integer fr, integer to)
begin
integer i,j;
	ClearArray(newArray);
	j=0;
	for(i=fr;i<=to;i=i+1) begin
		if (nonblank(MotherArray[i])) then begin
			newArray[j] = MotherArray[i];
			j=j+1;
		end;
	end;

return;
end;

