procedure NPUpdateENstatusMn(record RcVc RepSpec)
begin
  area request,replyarea,mainRequest;
  boolean TrHs;
  string 200 getParam,host,page;
  xml xdata;
  longint i,k;
  record NPSettingsBlock NPSettingsb;
  record SHVc SHr,oldSHr;
  array string 50 documents,tempArray;
  array longint deliveries;
  vector string 50 statuses;
  string 200 docNumber,status;
  integer steps;
  integer repeat;
	boolean testf,ssl;
  string 20 port;
  record ORVc ORr;

  repeat = 0;
	BlockLoad(NPSettingsb); 
  if (blank(NPSettingsb.APIkey)) then begin
  	goto LNPUpdateENstatusMn;
  end;	 
  host = "api.novaposhta.ua";
  page = "/v2.0/xml/";
  port = "443";
  ssl = true;
  i = 0;
  TrHs = true;
  
  SHr.NPIntDocNumber= "000";
  While(LoopKey("NPIntDocNumber",SHr,1,TrHs)) begin
  	if (blank(SHr.NPIntDocNumber)) then begin TrHs = false; end;
		if (TrHs and (SHr.NPStatusCode==0)) then begin
				ORr.SerNr = SHr.OrderNr;
				readfirstmain(ORr,1,true);
				if(ORr.OrderClass=="WEB")then begin
					documents[i] = SHr.NPIntDocNumber;
					i=i+1;
				end;
			end;  
  end;
   
	steps = documents.length / 10;
	
	for(k=0;k<=steps;k=k+1) begin
		
		ArrayFromArray(documents,tempArray,k*10,(k+1)*10-1);
		LsendRequestAgainGet2:;

  	SetAreaZeroSize(mainRequest);
		CreateDescriptRequestDocumentTrackig(host, page, NPSettingsb.APIkey, tempArray, mainRequest); 
  	
  	if(MySendRawWebRequest(host,port,ssl,"POST",page & getParam,"text/xml","",false,mainRequest,replyarea,5)) then begin  
  	  writeareatofile(replyarea,"1statusesReply.txt",0);
  	  
  	  GetXMLArea(replyarea);
  	  
			xdata = ParseXMLArea(replyarea);
			i = 0;
  	
			while(XmlNodeExists(xdata,"root/data/item[" & i & "]/Barcode")) begin
				
				docNumber = XmlGet(xdata,"root/data/item[" & i & "]/Barcode");
				status = XmlGet(xdata,"root/data/item[" & i & "]/StateName");
				
				SHr.NPIntDocNumber = docNumber;
				if (ReadFirstKey("NPIntDocNumber",SHr,1,true)) then begin
						ORr.SerNr = SHr.OrderNr;
						readfirstmain(ORr,1,true);
						if(ORr.OrderClass=="WEB")then begin
							if (SHr.NPStatus != status) then begin
								RecordCopy(oldSHr,SHr);
								SHr.NPStatus = status;
								if (status == USetStr(31063)) then begin
									SHr.NPStatusCode = 1;
								end;
								RecordUpdate(oldSHr,SHr,false);
								UpdateORVcNewPostStatuses(SHr);
							end;
						end;
				end;			
				i = i + 1;
			end;
			
		end else begin
			repeat = repeat + 1;
			if(repeat < 3) then begin
				millisleep(100);
				
				if(repeat==2)then begin
					host = "api.novaposhta.ua";
					page = "/v2.0/xml/";
					ssl = false;
					port = "80";
				end;
				Logtext(0,"Reply request NPUpdateENstatusMn " & repeat);
				goto LsendRequestAgainGet2;
			end;
			repeat = 0;					
			logtext(0,"Hansa World: No Connection to api.novaposhta.ua");
  	end;
  
  end;
  
  LNPUpdateENstatusMn:;
  return;
end;

