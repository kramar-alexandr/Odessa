		if(blank(RepSpec.f1))then begin
			if(CountRecords("NPWarehouseVc") > 0) then begin
				NPWr.CityRef = "";
				while (LoopMain(NPWr,1,true)) begin
					NPWr.synchFlag = 0;
					if (RecordStore(NPWr,true)) then begin		end;
				end;
			end;   
		end;
  
  
  	NPCr.City = "";
  	
  	if(nonblank(RepSpec.f1))then begin
  		NPCr.City = firstinrange(RepSpec.f1,20);
  	end;
  	TrHs = true;
		while (LoopMain(NPCr,1,TrHs)) begin
			testf = true;
			if(NPCr.City>lastinrange(RepSpec.f1,20) and nonblank(RepSpec.f1))then begin TrHs = false; testf = false; end;
			if(NPCr.City<firstinrange(RepSpec.f1,20) and nonblank(RepSpec.f1))then begin testf = false; end;
			
			
			if(TrHs and testf)then begin
				cityRef = NPCr.Ref;
				cityName = NPCr.City;
				
				LsendRequestAgainGet1:;
				SetAreaZeroSize(mainRequest);
						
				CreateWarehouseRequest(host, page, NPSettingsb.APIkey, "Address","getWarehouses", cityRef, mainRequest); 
				writeareatofile(mainRequest,"2getWarehousesRequest.txt",0);		
				
				if(MySendRawWebRequest(host,port,ssl,"POST",page & getParam,"text/xml","",false,mainRequest,replyarea,5))then begin 
					logtext(0,"Hansa World: GetWarehouses - " & cityName);
					writeareatofile(replyarea,"1getWarehousesReply.txt",0);
					GetXMLArea(replyarea);
					if (GetAreaLength(replyarea)==0) then begin	notSynchronize = true;  LogText(0,cityName);  goto L2222; 	end;
					xdata = ParseXMLArea(replyarea);
					NPParseErrors(xdata,errors);
					if (nonblank(errors)) then begin
						LogText(0,"Error " & errors);
						goto L2222;// Edit ************************** Friday, 15 April 2016 09:42:41
					end;				
					i = 0;
					while(XmlNodeExists(xdata,"root/data/item[" & i & "]/Description")) begin
						RecordNew(NPWr);
						descr = XmlGet(xdata,"root/data/item[" & i & "]/Description");
							NPWr.Description = descr;
							number = XmlGet(xdata,"root/data/item[" & i & "]/Number");	//Edit----------------------Dima  29.04.2015
							if (nonblank(number)) then begin 
								NPWr.Number = StringToInt(number);
							end; 
							NPWr.Ref = XmlGet(xdata,"root/data/item[" & i & "]/Ref"); 
							NPWr.DescriptionRu = XmlGet(xdata,"root/data/item[" & i & "]/DescriptionRu"); 
							NPWr.TypeOfWarehouse = XmlGet(xdata,"root/data/item[" & i & "]/TypeOfWarehouse"); 
							NPWr.CityRef = cityRef; 
							NPWr.Longitude = XmlGet(xdata,"root/data/item[" & i & "]/Longitude"); 
							NPWr.Latitude = XmlGet(xdata,"root/data/item[" & i & "]/Latitude"); 
							NPWr.City = cityName;
							NPWr.synchFlag = 1;
							if (RecordStore(NPWr,true)) then begin		end;
						i = i + 1;
					end;
				end else begin
					repeat = repeat + 1;
					if(repeat < 3) then begin
						millisleep(100);
						
						if(repeat==2)then begin
							host = "api.novaposhta.ua";
							page = "/v2.0/xml/AddressGeneral/getWarehouses";
							ssl = false;
							port = "80";
						end;
						goto LsendRequestAgainGet1;
					end;
					if (repeat>=3) then begin notSynchronize = true;LogText(0,cityName);	  end;
					repeat = 0;
					logtext(0,"Hansa World: No Connection to api.novaposhta.ua. There is no "  & cityName);
				end;				
L2222:;// Edit ************************** Friday, 15 April 2016 09:42:41

			end;
		end;
		
		if(blank(RepSpec.f1))then begin
			if (notSynchronize==false) then begin
				TrHs = true;
				Resetloop(NPWr);
				NPWr.synchFlag = 0;
				i = 0;
				while (LoopKey("synchFlag",NPWr,1,TrHs)) begin
					if (NPWr.synchFlag!=0) then begin TrHs = false; end;
					if (TrHs) then begin
						RecordDelete(NPWr);
						StepBack(NPWr);
						i=i+1;
					end;
				end;
				LogText(0,"deleted " & i);
			end;
		end;
		
LNPGetWarehouseMn:;
  return;
end;


