globalprocedure SumUpPackList(var record PackListVc PackListr)begin    row PackListVc PackListrw;  integer rwcnt,i;  val totvol,totweight;    totvol = 0;    totweight = 0;    rwcnt = MatRowCnt(PackListr);    for (i=0;i<rwcnt;i=i+1) begin      MatRowGet(PackListr,i,PackListrw);      if (NonBlank(PackListrw.Volume) and NonBlank(PackListrw.Weight)) then begin        totvol = totvol + PackListrw.Volume;        totweight = totweight + PackListrw.Weight;      end;     end;        PackListr.TotVolume = totvol;    PackListr.TotWeight = totweight;   return;end;