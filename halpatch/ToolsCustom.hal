global
updating procedure CityVcFromNPCityVcMn(record RcVc RepSpec)	
begin
  record NPCityVc np;
  record CityVc c;
  
  while(LoopMain(np,1,true))begin
    RecordNew(c);
    c.NameUKR = np.City;
    if !readfirstkey("NameUKR",c,1,true) then begin
      c.NameUKR = np.City;
      c.Name = np.CityRU;
      RecordStore(c,true);
    end;
  end;
  
  return;
end;




global procedure CalcCustBalrem(var record ORVc ORr)
