global updating procedure SetItemFlagsMn(record RcVc RepSpec)
begin
	record INVc INr;
	
	while(loopmain(INr,1,true))begin
		INr.ExpFlag01 = 1;
		INr.FExpFlag01 = 1;
		recordstore(INr,true);
	end;

return;
end;


global updating procedure DelItemStatusVc()
begin
	record ItemStatusVc ISr;
	record PISVc PISr;
	
	while(loopmain(ISr,1,true))begin
		recorddelete(ISr);
		stepback(ISr);
	end;
	
	while(loopmain(PISr,1,true))begin
		recorddelete(PISr);
		stepback(PISr);
	end;


return;
end;

global updating procedure UpASMn()
begin
record CUVc CUr;
record CUVc oldCUr;
boolean TrHs;
TrHs=true;
CUr.Code ="";
while(LoopMain(CUr,1,TrHs)) begin
	if(CUr.CustCat=="CUST" and CUr.CUType==1)then begin
		if(CUr.AgeStatus==0) then begin CUr.ShopF=1; end;
		if(CUr.AgeStatus==1) then begin CUr.MarketF=1; end;
		if(CUr.AgeStatus==2) then begin CUr.ShopF=1;CUr.MarketF=1; end;
		RecordStore(CUr,true);
	end;
	
end;
return;
end;	
	
	
	
	
	