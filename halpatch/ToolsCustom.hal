global updating procedure UpdateLastShipDateMn()//Edit----------------------Dima  31.08.2015
begin
	record ORVc ORr;
	record SHVc SHr,SHprev;
	date lastShipDate;
	boolean TrHs;
	longint previousOrder;


	TrHs = true;	
	While(LoopKey("OrderKey",SHr,1,TrHs)) begin
		if(SHr.OrderNr != previousOrder) then begin
				 ORr.SerNr = previousOrder;
				 if (ReadFirstMain(ORr,1,true)) then begin
						ORr.LastShipDate = lastShipDate;
						RecordStore(ORr,true);
				 end;
				 previousOrder = SHr.OrderNr;
				 lastShipDate = ""; 
		end;		

		if (SHr.ShipOkDate > lastShipDate) then begin
			lastShipDate = SHr.ShipOkDate;
		end;

	end;
  
end;





