global updating procedure SendBirthdaySMS()
begin
	record RebateVoucherBlock RebVchB;
  record RebateVoucherVc RebateVoucherr,RebateVoucher2r;
  record CUVc CUr;
  Boolean TrHs,testf,voucherExists;
  date curDate;
  LongInt newVoucherNr,smsNr;
  string 200 response;
  integer quant;
  date nextCUDR,targetDRDate;
  integer daybefore,dayto;
  string 255 smstext;
  record SMSTextBlock SMSb;
  record SMSTextOptBlock SMSOb;

  	blockload(SMSb);
  	blockload(SMSOb);
		blockload(RebVchB);
		if(RebVchB.ActivateSystem>0)then begin
			if (NonBlank(RebVchB.RebCode)) then begin
				curDate = CurrentDate;
				TrHs = true;
				while (LoopMain(CUr,1,TrHs)) begin
					nextCUDR = stringtodate(getday(CUr.BirthDate) & "/" & getmonth(CUr.BirthDate) & "/" & getyear(Currentdate));
					if(stringtodate(getday(CUr.BirthDate) & "/" & getmonth(CUr.BirthDate) & "/" & getyear(Currentdate))<currentdate)then begin
						nextCUDR = stringtodate(getday(CUr.BirthDate) & "/" & getmonth(CUr.BirthDate) & "/" & getyear(Currentdate)+1);
					end;
					testf = true;
					if (BlankDate(CUr.BirthDate)) then begin testf = false; end;
					if (curDate!=nextCUDR) then begin testf = false; end;
					if(RebVchB.SendSMSBeforeBirthDayAtTime>currenttime)then begin testf = false; end;
				
					/*if(CUr.CustCat=="CUST")then begin
						testf = false;
					end;*/
					if(CUr.CustCat!="EMPL")then begin
						testf = false;
					end;
					if (testf) then begin
						RebateVoucher2r.CustCode = CUr.Code;
						RebateVoucher2r.ReleasedAtYear = GetYear(curDate);
						testf = ReadFirstKey("CustCode",RebateVoucher2r,2,true);
					end;
					
					if (testf) then begin
						smstext = RebVchB.SMSText2;
						smstext = StrReplace(smstext,"@NR",RebateVoucher2r.VoucherSerNr);
						smstext = StrReplace(smstext,"@FR",RebateVoucher2r.IsValidFromDate);
						smstext = StrReplace(smstext,"@TO",RebateVoucher2r.IsValidToDate);
						smsNr = AddSMStoStack(CUr.Phone,SMSb.Title1,"",smstext,CUr.Code,"RebateVoucherVc",RebateVoucher2r.SerNr,"WEB",curDate,RebVchB.SendSMSBeforeBirthDayAtTime);
												
					end;
				end;
			end;
		end;
	
return;
end;


