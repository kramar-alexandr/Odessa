			For(i=0;i<mtrw;i=i+1) begin
	  		matrowget(ORr,i,ORrw);
	  		if((ORrw.Quant-ORrw.Shipd2)>0)then begin
	  			logtext(0,ORr.SerNr & "	" & ORrw.ArtCode & "	" & ORrw.Quant-ORrw.Shipd2);
	  			
	  			ISr.Location = ORr.Location;
	  			ISr.Code = ORrw.ArtCode;
	  			if(readfirstmain(ISr,2,true))then begin
	  				if(ISr.RsrvQty>=(ORrw.Quant-ORrw.Shipd2))then begin
	  					logtext(0,"ISr.RsrvQty1 " & ISr.RsrvQty);
	  					ISr.RsrvQty = ISr.RsrvQty - (ORrw.Quant-ORrw.Shipd2);
	  					logtext(0,"ISr.RsrvQty2 " & ISr.RsrvQty);
	  				end;
	  			end;
	  			recordstore(ISr,true);
	  			ISr.Location = ";;;";
	  			ISr.Code = ORrw.ArtCode;
	  			if(readfirstmain(ISr,2,true))then begin
	  				if(ISr.RsrvQty>=(ORrw.Quant-ORrw.Shipd2))then begin
	  					logtext(0,"ISr.RsrvQty1 " & ISr.RsrvQty);
	  					ISr.RsrvQty = ISr.RsrvQty - (ORrw.Quant-ORrw.Shipd2);
	  					logtext(0,"ISr.RsrvQty2 " & ISr.RsrvQty);
	  				end;
	  			end;
	  			recordstore(ISr,true);
	  			
	  			foundf = true;
	  		end;
			end; 
			if(foundf)then begin
				ORr.Reserved = 0;
				recordstore(ORr,true);
			end;
		end;
	end;

return;
end;

global updating procedure CreateLoyaltyCardName()
begin
