global updating procedure StockMovErrormn()
begin
	record ItemHistVc IHr;
	record StockMovVc SMr;
	row StockMovVc SMrw;
	integer i,mtrw;
	record INVc INr;
	boolean changef;
	
	SMr.SerNr = 15915;
	while(loopmain(SMr,1,true))begin
		changef = false;
		if(SMr.OKFlag>0)then begin
			mtrw = matrowcnt(SMr);
			For(i=0;i<mtrw;i=i+1) begin
				matrowget(SMr,i,SMrw);
				if(SMrw.OrdQuant>0 and (SMrw.SentQuant!=0 or SMrw.Quant!=0))then begin
					IHr.FileName = "StockMovVc";
					IHr.TransNr = SMr.SerNr;
					IHr.Row = i;
					if(readfirstkey("FNTransNr",IHr,3,true))then begin
						if(IHr.ArtCode!=SMrw.ArtCode)then begin
							logtext(0,"IH not ident SM# " & SMr.SerNr & " row # " & i+1 & " item " & SMrw.ArtCode);
							SMrw.ArtCode = IHr.ArtCode;
							INr.Code = SMrw.ArtCode;
							readfirstmain(INr,1,true);
							SMrw.Spec = INr.Name;
							matrowput(SMr,i,SMrw);
							changef = true;
						end;
					end else begin
						logtext(0,"IH not found for SM# " & SMr.SerNr & " row # " & i+1 & " item " & SMrw.ArtCode);
					end;
				end;
			end; 
		end;
		
		if(changef)then begin
			recordstore(SMr,true);
		end;
		
	end;
	
return;
end;

