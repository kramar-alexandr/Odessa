function val Min(val v1,val v2)
begin
  val res;
  
  if (v1>v2) then begin
    res = v2;
  end else begin
    res = v1;
  end;
  Min = res;
  return;
end;

global procedure CollectOrdersForStockMove(var record StockMovVc SMr, record RcVc RepSpec)	//Edit----------------------Dima  20.08.2015
begin
	record ORVc ORr;
	row ORVc ORrw;
	row StockMovVc SMrw;
	boolean TrHs,testf;
	integer rwcnt,i,j;
	longint pos;
	array string 255 aWarning;
	string 10 str1,str2;
	date sStartDate,sEndDate;
	
	j=0;
	pos = 0;
	GetNextSubstring(RepSpec.Period2Str,pos,":",str1);
	GetNextSubstring(RepSpec.Period2Str,pos,":",str2);
	sStartDate = StringToDate(str1);
	sEndDate = StringToDate(str2);
	
	SMr.SerNr = NextSerNr("StockMovVc",CurrentDate,-1,false,"");
	SMr.FrLocation = RepSpec.f1;
	SMr.ToLocation = RepSpec.f2;
	
	
	ORr.OrdDate = sStartDate;
	TrHs = true;
	While(LoopKey("OrdDate",ORr,1,TrHs)) begin
		testf = true;
		if (ORr.OrdDate > sEndDate) then begin TrHs = false; testf=false; end;
		if (RepSpec.f2 != ORr.Location) then begin testf=false; end;
		if (ORr.OKFlag>0 or ORr.Closed>0 or ORr.Reserved==0) then begin testf=false; end;
		if (ORr.CustomStatusFlag>5 and ORr.CustomStatusFlag<12)	then begin testf=false; end; //search only for active orders		
		if (testf) then begin	
			rwcnt = MatRowCnt(ORr);
			for(i=0;i<rwcnt;i=i+1)  begin
				MatRowGet(ORr,i,ORrw);
				if (ORrw.OrderQuant - ORrw.ProcessingQuant > 0) then begin
				
				LogText(0,"logistics creation: " & SMr.SerNr & "  OR:" & ORr.SerNr & "  code " & ORrw.ArtCode & "  ord:" & ORrw.OrderQuant & "  proc:" & ORrw.ProcessingQuant);
				
					SMrw.ArtCode = ORrw.ArtCode;
					SMrw.OrdQuant = ORrw.OrderQuant - ORrw.ProcessingQuant;
					SMrw.ORSerial = ORr.SerNr;
					SMrw.ORRowUID = ORrw.UID;
					MatRowPut(SMr,j,SMrw);
					StockMovVc_PasteArtCode(SMr,j,1,aWarning);
					j=j+1;
				end;			
			end;
		
		end;

	end;

end;


global updating procedure AddOrderForStockMove(var record StockMovVc SMr,var record ORVc ORr,boolean inser)// Edit ************************** Monday, 6 March 2017 11:21:47
begin
	row ORVc ORrw;
	row StockMovVc SMrw;
	boolean TrHs,testf;
	integer rwcnt,i,j;
	longint pos;
	array string 255 aWarning;
	string 10 str1,str2;
	date sStartDate,sEndDate;
	integer smrows;
	record StockMovVc oldSMr;
	record ItemStatusVc ISr;
	vector val artcodeinstock,artcodeneed;
	boolean addrow;
	record ORVc oldORr;
	record PISVc PISr;
	boolean mustreserv;
	
	
	j=0;
	pos = 0;
	
	recordcopy(oldORr,ORr);
	
	rwcnt = MatRowCnt(ORr);
	for(i=0;i<rwcnt;i=i+1)  begin
		MatRowGet(ORr,i,ORrw);
		artcodeneed[ORrw.ArtCode] = artcodeneed[ORrw.ArtCode] + ORrw.OrderQuant;
		ISr.Code = ORrw.ArtCode;
		ISr.Location = SMr.FrLocation;
		if(readfirstmain(ISr,2,true))then begin
			artcodeinstock[ORrw.ArtCode] = ISr.Instock - ISr.RsrvQty;
			if(SMr.FrLocation=="3-RAY" and SMr.FrPos=="4-NIZ")then begin
				PISr.ArtCode = ORrw.ArtCode;
				PISr.Location = SMr.FrLocation;
				PISr.Position = "4-NIZ";
				if(readfirstmain(PISr,3,true))then begin
					if(PISr.Instock>ISr.RsrvQty and artcodeinstock[ORrw.ArtCode]>0)then begin
						artcodeinstock[ORrw.ArtCode] = PISr.Instock - ISr.RsrvQty;
					end else begin
						artcodeinstock[ORrw.ArtCode] = 0;
					end;
				end else begin
					artcodeinstock[ORrw.ArtCode] = 0;
				end;
			end;
		end;
	end;
	
	mustreserv = false;
	if(inser==false)then begin
		recordcopy(oldSMr,SMr);
		if(SMr.Reserved>0)then begin
			mustreserv = true;
			SMr.Reserved = 0;
			recordupdate(oldSMr,SMr,true);
		end;
	end;
	smrows = matrowcnt(SMr);
	rwcnt = MatRowCnt(ORr);
	addrow = false;
	recordcopy(oldSMr,SMr);
	for(i=0;i<rwcnt;i=i+1)  begin
		MatRowGet(ORr,i,ORrw);
		if (ORrw.OrderQuant - ORrw.ProcessingQuant > 0 and artcodeinstock[ORrw.ArtCode]>0) then begin
			addrow = true;
			LogText(0,"Auto logistics creation: " & SMr.SerNr & "  OR:" & ORr.SerNr & "  code " & ORrw.ArtCode & "  ord:" & ORrw.OrderQuant & "  proc:" & ORrw.ProcessingQuant);
		
			SMrw.ArtCode = ORrw.ArtCode;
			MatRowPut(SMr,smrows,SMrw);
			StockMovVc_PasteArtCode(SMr,smrows,1,aWarning);
			MatRowGet(SMr,smrows,SMrw);
			if((ORrw.OrderQuant - ORrw.ProcessingQuant)<=artcodeinstock[ORrw.ArtCode])then begin
				SMrw.OrdQuant = ORrw.OrderQuant - ORrw.ProcessingQuant;
				if(inser==false)then begin
					SMrw.OrderProcessing = ORrw.OrderQuant - ORrw.ProcessingQuant;
					ORrw.ProcessingQuant = ORrw.ProcessingQuant + SMrw.OrdQuant;
					ORrw.SMReserved = ORrw.SMReserved & SMr.SerNr & ",";
					matrowput(ORr,i,ORrw);
				end;
			end else begin
				SMrw.OrdQuant = artcodeinstock[ORrw.ArtCode];
				if(inser==false)then begin
					SMrw.OrderProcessing = artcodeinstock[ORrw.ArtCode];
					ORrw.ProcessingQuant = ORrw.ProcessingQuant + SMrw.OrdQuant;
					if(nonblank(ORrw.SMReserved))then begin
						if(setinset(SMr.SerNr,ORrw.SMReserved)==false)then begin
							ORrw.SMReserved = ORrw.SMReserved & SMr.SerNr & ",";
						end;
					end else begin
						ORrw.SMReserved = ORrw.SMReserved & SMr.SerNr & ",";
					end;
					matrowput(ORr,i,ORrw);
				end;
			end;
			SMrw.ORSerial = ORr.SerNr;
			SMrw.ORRowUID = ORrw.UID;
			MatRowPut(SMr,smrows,SMrw);
			
			smrows=smrows+1;
			addrow = true;
		end;			
	end;
	
	if(inser==false and addrow)then begin		
		if(SMr.Reserved==0)then begin
			if(mustreserv)then begin
				SMr.Reserved = 1;
			end;
			//SMr.TransDate = Currentdate;// Edit ************************** BPI Ukraine - KramarAlexandr - Tuesday, 16 October 2018 10:08:35
			if(matrowcnt(SMr)>199)then begin
				SMr.StopAutoComplite = 1;// Edit ************************** BPI Ukraine - KramarAlexandr - Wednesday, 3 January 2018 10:14:15
			end;
			recordupdate(oldSMr,SMr,true);
			CreateRecordLink(SMr,CurrentCompany,ORr,CurrentCompany);
		 	CreateRecordLink(ORr,CurrentCompany,SMr,CurrentCompany);
		end;
		recordupdate(oldORr,ORr,true);
	end else begin
		//recordupdate(oldORr,ORr,true);
		if(mustreserv)then begin
			SMr.Reserved = 1;
		end;
		if(matrowcnt(SMr)>199)then begin
			SMr.StopAutoComplite = 1;// Edit ************************** BPI Ukraine - KramarAlexandr - Wednesday, 3 January 2018 10:14:15
		end;
		recordinsert(SMr,true);
	end;
end;

