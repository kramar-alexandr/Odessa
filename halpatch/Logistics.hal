	vector longint matches;
	
	rwcnt = MatRowCnt(SMr);	
	
	for(i=0;i<rwcnt;i=i+1) begin
		MatRowGet(SMr,i,SMrw);
		if (SMrw.ORSerial>0) then begin
			//LogText(0,"row reading   " & SMrw.ORSerial);
