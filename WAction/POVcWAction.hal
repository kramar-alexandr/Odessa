function Boolean PODClassInsertRowTest(Integer wn,Integer rownr)
begin
  record POVc POr;
  record POVc PO2r;
  Boolean res;

  res = false;  
  GetWindowRecord(wn,POr);
  if (POr.Closed==0) then begin
    res = true;  
    if (POAcceptanceStarted(POr) and UserCanAction("ChangeRecordMatrixWhenPending",false)==false) then begin
