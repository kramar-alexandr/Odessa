function Boolean PODClassDeleteRowTest(Integer wn,Integer rownr)
begin
  record POVc POr;
  record POVc PO2r;
  Boolean res;

  res = true;
  GetWindowRecord(wn,POr);
  if (POr.OrderType==kOrderTypeDropShip) then begin 
    res = false;
