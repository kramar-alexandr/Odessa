59 10.03.2017		NewLine;	while(loopmain(CUr,1,true))begin		ExportString(CUr.Code);		ExportString(CUr.CustCat);		ExportString(CUr.RebCode);		ExportString(CUr.Name);		ExportString(CUr.AgeStatus);		ExportString(CUr.InvAddr0);		ExportString(CUr.InvAddr1);		ExportString(CUr.InvAddr2);		ExportString(CUr.InvAddr3);		ExportString(CUr.InvAddr4);		ExportString(CUr.DelAddr0);		ExportString(CUr.DelAddr1);		ExportString(CUr.DelAddr2);		ExportString(CUr.CountryCode);		ExportString(CUr.Department);				if(nonblank(oblast[CUr.Region]))then begin			ExportString(oblast[CUr.Region]);		end else begin			ExportString(CUr.Region);		end;		ExportString(CUr.Fax);		ExportString(CUr.Ethnicity);		ExportString(CUr.SkypeName);		ExportString(CUr.Phone);		ExportString(CUr.AltPhone);		ExportString(CUr.Extension);		ExportString(CUr.eMail);		ExportString(CUr.Comment0);		ExportString(CUr.Comment1);		ExportString(CUr.Comment2);		ExportString(CUr.Person);		ExportString(CUr.Mobile);		ExportString(CUr.wwwAddr);		ExportString(CUr.DateChanged);		ExportString(CUr.Classification);		ExportString(CUr.BirthDate);		ExportString(CUr.PassportNr);		ExportString(CUr.VATNr);		ExportString(CUr.MarStatus);		ExportString(CUr.Salutation1);		ExportString(CUr.Salutation2);		ExportString(CUr.Salutation3);		ExportString(CUr.DateCreated);		ExportString(CUr.VATNrCheckDate);		ExportString(CUr.MinOrdSum); //Edit***************************Sasha2,15:59 10.03.2017			NewLine;	end;			//endjob;return;end;