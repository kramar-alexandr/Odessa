external function LongInt TimeDiffInSeconds(Time,Time);external procedure ExtractObj(string,var Integer,var string);SetLangMode(LangRussian,"RUS",0);		global procedure ExpSalesStatEn(record RcVc RepSpec)beginrecord IVVc IVr;row IVVc IVrw;string 50 location;boolean TrHs,testf;integer i,mtrw,lines;	location = RepSpec.f1;		if(nonblank(location))then begin				IVr.Location = location;		TrHs = true;		While(loopkey("Location",IVr,1,true)) begin	  	testf = true;	  	if(IVr.Location!=location)then begin testf = false; TrHs = false; end;	  	if(IVr.OKFlag==0)then begin testf = false; end;	  	if(IVr.Invalid==1)then begin testf = false; end;	  	lines = 0;	  		  	if(testf)then begin	  		mtrw = matrowcnt(IVr);	  		For(i=0;i<mtrw;i=i+1) begin	  			matrowget(IVr,i,IVrw);	  			if(IVrw.stp==kInvoiceRowTypeNormal)then begin	  				if(IVrw.Sum!=0)then begin	  					lines = lines + 1;	  					if(IVrw.ArtCode=="9701185")then begin lines = lines - 1; IVr.Sum4 = IVr.Sum4 - IVrw.Sum; end;	  					if(IVrw.ArtCode=="9701186")then begin lines = lines - 1; IVr.Sum4 = IVr.Sum4 - IVrw.Sum; end;	  					if(IVrw.ArtCode=="9701187")then begin lines = lines - 1; IVr.Sum4 = IVr.Sum4 - IVrw.Sum; end;	  					if(IVrw.ArtCode=="9701188")then begin lines = lines - 1; IVr.Sum4 = IVr.Sum4 - IVrw.Sum; end;	  					if(IVrw.ArtCode=="9701189")then begin lines = lines - 1; IVr.Sum4 = IVr.Sum4 - IVrw.Sum; end;	  					if(IVrw.ArtCode=="9701191")then begin lines = lines - 1; IVr.Sum4 = IVr.Sum4 - IVrw.Sum; end;	  					if(IVrw.ArtCode=="9701192")then begin lines = lines - 1; IVr.Sum4 = IVr.Sum4 - IVrw.Sum; end;	  					if(IVrw.ArtCode=="9701193")then begin lines = lines - 1; IVr.Sum4 = IVr.Sum4 - IVrw.Sum; end;	  				end;	  			end;				end; 	 				ExportPadString("\"" & location & "\",",len(location)+3,"",false);				ExportPadString("\"" & DateToString(IVr.InvDate,"YYYY/MM/DD") & "\",",len(DateToString(IVr.InvDate,"YYYY/MM/DD"))+3,"",false);				if(IVr.InvDate>stringtodate("11/05/2015"))then begin					ExportPadString(TimeDiffInSeconds(stringtotime("0:0:0"),addhours(IVr.TransTime,0)) & ",",len(TimeDiffInSeconds(stringtotime("0:0:0"),addhours(IVr.TransTime,0)))+1,"",false);				end else begin					ExportPadString(TimeDiffInSeconds(stringtotime("0:0:0"),addhours(IVr.TransTime,1)) & ",",len(TimeDiffInSeconds(stringtotime("0:0:0"),addhours(IVr.TransTime,1)))+1,"",false);				end;				ExportPadString(GetDateID(IVr.InvDate) & ",",len(GetDateID(IVr.InvDate))+1,"",false);				if(IVr.InvDate>stringtodate("11/05/2015"))then begin					ExportPadString(gethour(IVr.TransTime)+1 & ",",len(gethour(IVr.TransTime)+1)+1,"",false);				end else begin					ExportPadString(gethour(IVr.TransTime) & ",",len(gethour(IVr.TransTime))+1,"",false);				end;				ExportPadString(lines & ",",len(lines)+1,"",false);				ExportPadString(valtostring(IVr.Sum4,M4UVal,"",".",0),len(valtostring(IVr.Sum4,M4UVal,"",".",0)),"",false);				ExportPadString(chr(13) & chr(10),2,"",false);	  	end;		end; 	end;		//endjob;return;end;global procedure ExpItemsBarCodesEn(record RcVc RepSpec)beginrecord INVc INr;boolean TrHs,testf,foundf;integer i,mtrw,lines;record PLVc PLr;record ItemStatusVc ISr;val rrpprice;string 255 tstr;record DIVc DIr;record CTypeVc CTyper;integer pos;string 100 text;				setexportcodepage("UTF-8");		INr.Code = "";		TrHs = true;		While(loopmain(INr,1,true)) begin				  	testf = true;	  	if (INr.NotForSales!=0) then begin testf = false; end;	  		  	if(testf)then begin	  		testf = false;				ISr.Code = INr.Code;				ISr.Location = "3-RAY";				if(readfirstmain(ISr,2,true))then begin testf = true; end;				ISr.Code = INr.Code;				ISr.Location = "4-NIZ";				if(readfirstmain(ISr,2,true))then begin testf = true; end;				ISr.Code = INr.Code;				ISr.Location = "6-KV";				if(readfirstmain(ISr,2,true))then begin testf = true; end;				ISr.Code = INr.Code;				ISr.Location = "RYBRAY";				if(readfirstmain(ISr,2,true))then begin testf = true; end;	  		  	end;	  		  	rrpprice = blankval;	  	if(testf)then begin	  		foundf = true;	  		PLr.ArtCode = INr.Code;				PLr.PLCode = "RRP_A";				if(readfirstmain(PLr,2,true))then begin					foundf = false;					rrpprice = PLr.ExVatPrice;				end;	  			  		if(foundf)then begin					PLr.ArtCode = INr.Code;					PLr.PLCode = "RRP";					if(!readfirstmain(PLr,2,true))then begin						testf = false;					end else begin						rrpprice = PLr.ExVatPrice;					end;				end;									  	end;	  		  	if(testf)then begin				ExportPadString(INr.BarCode & ";",BytesInString(INr.BarCode)+1,"",false);				ExportPadString(INr.Code & ";",BytesInString(INr.Code)+1,"",false);				ExportPadString(INr.Name & ";",BytesInString(INr.Name)+1,"",false);				/*PLr.ArtCode = INr.Code;				PLr.PLCode = "RRP";				readfirstmain(PLr,2,true);*/				ExportPadString(";",1,"",false);				ExportPadString(rrpprice & ";",len(rrpprice)+1,"",false);				//ExportPadString(INr.UserStr1 & ";",BytesInString(INr.UserStr1)+1,"",false);								pos = 0;				ExtractObj(INr.WebDispGroups,pos,text);				while(nonblank(text))begin					if(nonblank(text))then begin						DIr.Code = text;						if(readfirstmain(DIr,1,true))then begin							switch(DIr.CType)begin								case "BRND_UKR":								case "BRND_WORD":								case "TYPE":								case "VENDOR":								case "ПОСТАВЩИК":								otherwise								CTyper.Code = DIr.CType;								if(readfirstmain(CTyper,1,true))then begin									tstr = CTyper.Comment & " " & DIr.Name & "\\r";									ExportPadString(tstr,BytesInString(tstr),"",false);								end;							end;						end;					end;					ExtractObj(INr.WebDispGroups,pos,text);				end;																				ExportPadString(";" & chr(13) & chr(10),3,"",false);	  	end;		end; return;end;global procedure ExpItemsBarCodeOptsEn(record RcVc RepSpec)beginrecord INVc INr;boolean TrHs,testf;integer i,mtrw,lines;record PLVc PLr;record ItemStatusVc ISr;val rrpprice,optprice;string 255 tstr;record DIVc DIr;record CTypeVc CTyper;integer pos;string 100 text;		setexportcodepage("UTF-8");		INr.Code = "";		TrHs = true;		While(loopmain(INr,1,true)) begin	  	testf = true;	  	if (INr.NotForSales!=0) then begin testf = false; end;	  		  	if(testf)then begin	  		testf = false;	  		ISr.Code = INr.Code;				ISr.Location = "1-KOC";				if(readfirstmain(ISr,2,true))then begin 					if(ISr.Instock>0)then begin						testf = true;					end;				end;				ISr.Code = INr.Code;				ISr.Location = "3-RAY";				if(readfirstmain(ISr,2,true))then begin 					if(ISr.Instock>0)then begin						testf = true;					end;				end;				ISr.Code = INr.Code;				ISr.Location = "4-NIZ";				if(readfirstmain(ISr,2,true))then begin 					if(ISr.Instock>0)then begin						testf = true;					end;				end;				ISr.Code = INr.Code;				ISr.Location = "6-KV";				if(readfirstmain(ISr,2,true))then begin 					if(ISr.Instock>0)then begin						testf = true;					end;				end;				ISr.Code = INr.Code;				ISr.Location = "RYBRAY";				if(readfirstmain(ISr,2,true))then begin 					if(ISr.Instock>0)then begin						testf = true;					end;				end;	  	end;	  		  	//rrpprice = blankval;	  	optprice = blankval;	  		  	if(testf)then begin	  		PLr.ArtCode = INr.Code;				PLr.PLCode = "OPT";				if(!readfirstmain(PLr,2,true))then begin					testf = false;				end else begin					optprice = PLr.ExVatPrice;				end;	  	end;	  		  	if(testf)then begin				ExportPadString(INr.BarCode & ";",BytesInString(INr.BarCode)+1,"",false);				ExportPadString(INr.Code & ";",BytesInString(INr.Code)+1,"",false);				ExportPadString(INr.Name & ";",BytesInString(INr.Name)+1,"",false);				/*PLr.ArtCode = INr.Code;				PLr.PLCode = "OPT";				readfirstmain(PLr,2,true);*/				ExportPadString(optprice & ";",len(optprice)+1,"",false);				PLr.ArtCode = INr.Code;				PLr.PLCode = "RRP";				readfirstmain(PLr,2,true);				ExportPadString(PLr.ExVatPrice & ";",len(PLr.ExVatPrice)+1,"",false);				//ExportPadString(INr.UserStr1 & ";",BytesInString(INr.UserStr1)+1,"",false);				pos = 0;				ExtractObj(INr.WebDispGroups,pos,text);// Edit ************************** Friday, 8 April 2016 23:24:15				while(nonblank(text))begin					if(nonblank(text))then begin						DIr.Code = text;						if(readfirstmain(DIr,1,true))then begin							switch(DIr.CType)begin								case "BRND_UKR":								case "BRND_WORD":								case "TYPE":								case "VENDOR":								case "ПОСТАВЩИК":								otherwise								CTyper.Code = DIr.CType;								if(readfirstmain(CTyper,1,true))then begin									tstr = CTyper.Comment & " " & DIr.Name & "\\r";									ExportPadString(tstr,BytesInString(tstr),"",false);								end;							end;						end;					end;					ExtractObj(INr.WebDispGroups,pos,text);				end;				ExportPadString(";" & chr(13) & chr(10),3,"",false);	  	end;		end; return;end;global procedure ExpItemsBarCodeOpts1En(record RcVc RepSpec)beginrecord INVc INr;boolean TrHs,testf;integer i,mtrw,lines;record PLVc PLr;record ItemStatusVc ISr;val rrpprice,optprice;string 255 tstr;record DIVc DIr;record CTypeVc CTyper;integer pos;string 100 text;		setexportcodepage("UTF-8");		INr.Code = "";		TrHs = true;		While(loopmain(INr,1,true)) begin			i = 0;	  	testf = true;	  	if (INr.NotForSales!=0) then begin testf = false; end;	  		  	if(testf)then begin	  		ISr.Code = INr.Code;				ISr.Location = "1-KOC";				if(readfirstmain(ISr,2,true))then begin					if(ISr.Instock>0)then begin						i = 1;					end;				end;	  	end;	  		  	optprice = blankval;	  		  	if(testf)then begin	  		PLr.ArtCode = INr.Code;				PLr.PLCode = "RRP";				if(!readfirstmain(PLr,2,true))then begin					testf = false;				end else begin					optprice = PLr.ExVatPrice;				end;	  	end;	  		  	if(testf)then begin				ExportPadString(INr.Code & ";",BytesInString(INr.Code)+1,"",false);				ExportPadString(INr.Name & ";",BytesInString(INr.Name)+1,"",false);				ExportPadString(optprice & ";",len(optprice)+1,"",false);				ExportPadString(i & chr(13) & chr(10),3,"",false);	  	end;		end; return;end;global procedure ExportAllItems(record RcVc RepSpec)begin	record ExportStockCSVBlock ExportStockCSVb;	string 250 tstr;	string 100 serverip;		BlockLoad(ExportStockCSVb);	setfileonserver(true);		createfile("ItemsOPT.csv");		openexportfile("ItemsOPT.csv",false);			ExpItemsBarCodeOptsEn(RepSpec);		closefile;				createfile("ItemsRRP.csv");		openexportfile("ItemsRRP.csv",false);			ExpItemsBarCodesEn(RepSpec);		closefile;				createfile("Stat3RAY.csv");			RepSpec.f1 = "3-RAY";			ExpSalesStatEn(RepSpec);		closefile;				createfile("Stat6KV.csv");			RepSpec.f1 = "6-KV";			ExpSalesStatEn(RepSpec);		closefile;						createfile("ItemsRRPOPT.csv");		openexportfile("ItemsRRPOPT.csv",false);			ExpItemsBarCodeOpts1En(RepSpec);		closefile;		if (FileExists("ItemsRRPOPT.csv")) then begin		  CopyFile("ItemsRRPOPT.csv","webcust/ItemsRRPOPT.csv");		end;				/*tstr = "188.40.105.5" & " " & "21" & " ";		tstr = tstr & "mobileapp" & " ";		tstr = tstr & "5UTp4G4c" & " ";		tstr = tstr & "/" & " " & "ItemsOPT.csv ItemsRRP.csv Stat3RAY.csv Stat6KV.csv";		RunProgram("./ftptransfercsv.sh", tstr);*/				serverip = ExportStockCSVb.HostIP;				tstr = serverip & " " & "21" & " ";		tstr = tstr & "mobile" & " ";		tstr = tstr & "oRief0ei3A" & " ";		tstr = tstr & "files/" & " " & "ItemsOPT.csv";		RunProgram("./ftptransfer.sh", tstr);		tstr = serverip & " " & "21" & " ";		tstr = tstr & "mobile" & " ";		tstr = tstr & "oRief0ei3A" & " ";		tstr = tstr & "files/" & " " & "ItemsRRP.csv";		RunProgram("./ftptransfer.sh", tstr);		tstr = serverip & " " & "21" & " ";		tstr = tstr & "mobile" & " ";		tstr = tstr & "oRief0ei3A" & " ";		tstr = tstr & "files/" & " " & "Stat3RAY.csv";		RunProgram("./ftptransfer.sh", tstr);		tstr = serverip & " " & "21" & " ";		tstr = tstr & "mobile" & " ";		tstr = tstr & "oRief0ei3A" & " ";		tstr = tstr & "files/" & " 