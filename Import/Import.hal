=CUr.Extension)then begin	  			if(ORrw.Spec!=CUr.eMail)then begin	  				CUr.eMail = ORrw.Spec;	  				recordstore(CUr,true);	  			end;	  		end;			end; 		end;	end;return;end;global //Edit	by Victor 14.08.15updating procedure ImportCUDiscountIn()beginstring 100 email,group_id;record CUVc CUr;boolean TrHs,testf1;	while (TestEOF()==false) begin		email = ImportField;		group_id = ImportField;		if(group_id == "1" or group_id == "2") then begin			goto LImportCUDiscountIn;		end else			begin				if(nonblank(email))then begin					CUr.eMail = email;					if(readfirstkey("eMail",CUr,1,true))then begin						if(group_id == "6") then begin							CUr.RebCode = "5%";							recordStore(CUr,true);						end else begin							if(group_id == "5") then begin								CUr.RebCode = "10%";								recordStore(CUr,true);							end else begin 								if(group_id == "4") then begin									CUr.RebCode = "15%";									recordStore(CUr,true);								end;							end;						end;					end;				end;			end;	LImportCUDiscountIn:;	NextImportLine(true);       end; 	return;end;