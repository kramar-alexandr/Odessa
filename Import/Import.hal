bal updating procedure UpdateCUEmailsIn()begin	string 100 email,number;	integer k,i;	record ORVc ORr;	row ORVc ORrw;	record CUVc CUr;		while (TestEOF()==false) begin		email = ImportField;		number = ImportField;		ORrw.Spec = email;		ORrw.ArtCode = number;		matrowput(ORr,k,ORrw);		k=k+1;		NextImportLine(true);	end;		CUr.Code = "";	while(loopmain(CUr,1,true))begin		if(nonblank(CUr.Extension))then begin			For(i=0;i<k;i=i+1) begin	  		matrowget(ORr,i,ORrw);	  		if(ORrw.ArtCode==CUr.Extension)then begin	  			if(ORrw.Spec!=CUr.eMail)then begin	  				CUr.eMail = ORrw.Spec;	  				recordstore(CUr,true);	  			end;	  		end;			end; 		end;	end;return;end;global //Edit	by Victor 14.08.15updating procedure ImportCUDiscountIn()beginstring 100 email,group_id;record CUVc CUr;boolean TrHs,testf1;	while (TestEOF()==false) begin		email = ImportField;		group_id = ImportField;		if(group_id == "1" or group_id == "2") then begin			goto LImportCUDiscountIn;		end else			begin				if(nonblank(email))then begin					CUr.eMail = email;					if(readfirstkey("eMail",CUr,1,true))then begin						if(group_id == "6") then begin							CUr.RebCode = "5%";							recordStore(CUr,true);						end else begin							if(group_id == "5") then begin								CUr.RebCode = "10%";								recordStore(CUr,true);							end else begin 								if(group_id == "4") then begin									CUr.RebCode = "15%";									recordStore(CUr,true);								end;							end;						end;					end;				end;			end;	LImportCUDiscountIn:;	NextImportLine(true);       end; 	return;end;global updating procedure RestorePLHistIn()		//Edit----------------------Dima  11.02.2016beginrecord PLHistVc PLHistImprt,PLHistr;row PLHistVc PLHistrw,PLHistImprtrw;integer i,rwcnt;//restore difference between old version and current version of PLHistVc	while (TestEOF()==false) begin    RecordNew(PLHistImprt);    ImportOneRecord(PLHistImprt,"PriceHistory");		PLHistr.PLCode = PLHistImprt.PLCode;		PLHistr.ArtCode = PLHistImprt.ArtCode;		//PLHistr.CustCode = PLHistImprt.CustCode; //don't used in RybRay		if (ReadFirstMain(PLHistr,2,true)) then begin			MatRowGet(PLHistr,0,PLHistrw);			if (BlankDate(PLHistrw.ChDate) and BlankTime(PLHistrw.ChTime))	 then begin				 MatRowDelete(PLHistr,0);			end;			MatRowGet(PLHistr,0,PLHistrw);			for (i=MatRowCnt(PLHistImprt)-1;i>=0;i=i-1) begin				MatRowGet(PLHistImprt,i,PLHistImprtrw);				if ((PLHistImprtrw.ChDate < PLHistrw.ChDate) or (PLHistImprtrw.ChDate == PLHistrw.ChDate and PLHistImprtrw.ChTime < PLHistrw.ChTime)) then begin					MatRowInsert(PLHistr,0,PLHistImprtrw);				end;			end;			RecordStore(PLHistr,true);		end else begin			RecordStore(PLHistImprt,true);		end;		NextImportLine(true);       end; return;end;globalupdating procedure ImportCloseItemsIn()begin  string 100 artcode;  record INVc INr;	while (TestEOF()==false) begin		artcode = ImportField;				if (nonblank(artcode)) then begin			INr.Code = artcode;			if (readfirstmain(INr,1,true)) then begin        INr.Terminated = 1;        recordStore(INr,true);			end;		end;			NextImportLine(true);       end; 	  return;end;globalupdating procedure ImportOpenItemsIn()begin  string 100 artcode;  record INVc INr;	while (TestEOF()==false) begin		artcode = ImportField;				if (nonblank(artcode)) then begin			INr.Code = artcode;			if (readfirstmain(INr,1,true)) then begin        INr.Terminated = 0;        recordStore(INr,true);			end;		end;			NextImportLine(true);       end; 	  return;end;globalupdating procedure ChangeClassificationNameIn()begin  string 20 diCode;  string 255 diName;  record DIVc DIr;	while (TestEOF()==false) begin		diCode = ImportField;		diName = ImportField;				if (nonblank(diCode) and nonblank(diName)) then begin			DIr.Code = diCode;			if (readfirstmain(DIr,1,true) and DIr.Name!=diName) then begin        LogText(0,DIr.Code & "!!!!" & diName);        DIr.Name = diName;        RecordStore(DIr,true);			end;		end;		NextImportLine(true);   end; 	  return;end;