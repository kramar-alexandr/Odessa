			total = total + ConsSalerw.SelPrice*ConsSalerw.SelQty;
			EndFormat;	
			cnt = cnt + 1;
		end;
		StartFormat(15);
			OutString(0,0,"",true);
			OutString(2,0,"",true);
			OutString(3,0,"",false);
			OutString(4,0,"Всего",false);
