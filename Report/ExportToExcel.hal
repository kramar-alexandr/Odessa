	SetRepCol(12,550);
	SetRepCol(13,600);


	StartFormat(15);
	OutString(0,0,"№",true);
	OutString(2,0,"Товар",true);
	OutString(3,0,"Кол-во",false);
	OutString(4,0,"Цена",false);
	OutString(5,0,"Сумма",false);
	EndFormat;
	Black_Divider(0,1);
	cnt = 1;
	for(i=0;i<rwcnt;i=i+1) begin
