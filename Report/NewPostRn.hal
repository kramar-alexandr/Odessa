SetLangMode(LangRussian,"RUS",0);globalprocedure ENGetStatusRn(record RcVc RepSpec)	//Edit-------------------------Dima 04.12.2014begin	record SHVc SHr;	date startDate,endDate;	boolean TrHs;	string 200 response;			TrHs = true;	startDate = RepSpec.sStartDate;	endDate = RepSpec.sEndDate;	SHr.ShipDate = startDate;	StartReportNoHeaderJob("Статусы электронных накладных");	StartFormat(15);		OutString(0,0,"№ документа",false);		OutString(100,0,"№ ЭН",false);		OutString(200,0,"Статус",false);		EndFormat;		SHr.ShipDate = startDate;	While(LoopKey("ShipDate",SHr,1,TrHs)) begin		if (SHr.ShipDate > endDate) then begin TrHs = false; end;		if (TrHs) begin			if (nonblank(SHr.NPIntDocNumber)) then begin					StartFormat(15);						OutString(0,0,SHr.SerNr,false);						OutString(100,0,SHr.NPIntDocNumber,false);						OutString(200,0,SHr.NPStatus,false);						EndFormat;			end;		end;	end;		EndJob;end;