							PO2rw.ArtCode = POrw.ArtCode;										if(POr.OKFlag==1)then begin											PO2rw.Quant = POrw.Quant;											PO2rw.Shipd1 = POrw.Quant - POrw.Shipd2;										end else begin											PO2rw.Price = POrw.Quant;											PO2rw.Shipd2 = POrw.Quant - POrw.Shipd2;										end;									end else begin											PO2rw.ArtCode = POrw.ArtCode;										if(POr.OKFlag==1)then begin											PO2rw.Quant = POrw.Shipd2;										end else begin											PO2rw.Price = POrw.Shipd2;										end;									end;									MatRowPut(PO2r,k,PO2rw);// Edit ************************** Monday, 4 August 2014 17:22:52									k=k+1;// Edit ************************** Monday, 4 August 2014 17:22:51								end;							end;						end;					end;				end;		end;	end;	return;end;*/ //Edit----------------------Dima  13.03.2015globalprocedure CollectPORows(var record POVc PO2r,Date sStartDate,Date sEndDate,string classification) BEGIN   integer i,j,rwcnt,rwcnt2,existsItem,k,pos;  Boolean TrHs,testf;  record POVc POr;  row POVc POrw,PO2rw;  longint ordOk,ordUnOk;  record DIVc DIr;  string 100 vendors,temp;    vendors = "";  pos = 0;  ExtractObj(classification,pos,temp);  while (nonblank(temp)) begin		  	DIr.Code = temp;  	ReadFirstMain(DIr,1,true);		if(DIr.CType=="VENDOR") then begin	//Edit----------------------Dima  19.03.2015			vendors = vendors & DIr.Name & ",";		end;   	  	ExtractObj(classification,pos,temp);  end;      k=0;// Edit ************************** Monday, 4 August 2014 17:22:46  ordOk = 0;	ordUnOk = 0;	POr.ActOkFlag = 1;	TrHs = true;	while (LoopKey("ActOkFlag",POr,1,TrHs)) begin	//Edit----------------------Dima  12.03.2015		testf = true;		if (POr.ActOkFlag==0) then begin TrHs = false; testf = false; end;		//if (nonblank(vendors) and (SetInSet(POr.VECode,vendors)==false)) then begin testf = false; end;				if (testf) then begin				rwcnt = MatRowCnt(POr);				if (rwcnt>0) then begin					for (i=0;i<rwcnt;i=i+1) begin						MatRowGet(POr,i,POrw);						if(nonblank(POrw.ArtCode) and POrw.stp==kInvoiceRowTypeNormal)then begin// Edit ************************** Monday, 4 August 2014 17:24:51							rwcnt2 = MatRowCnt(PO2r);																existsItem = 0;								if (rwcnt2>0) then begin									for (j=0;j<rwcnt2;j=j+1) begin										MatRowGet(PO2r,j,PO2rw);										if (PO2rw.ArtCode==POrw.ArtCode) then begin																							if(POr.OKFlag==1)then begin													PO2rw.Quant = PO2rw.Quant + POrw.Quant;													PO2rw.Shipd1 = PO2rw.Shipd1 + (POrw.Quant - POrw.Shipd2);													ordOk = POrw.Quant - POrw.Shipd2;												end else begin													PO2rw.Price = PO2rw.Price + POrw.Quant;													PO2rw.Shipd2 = PO2rw.Shipd2 + (POrw.Quant - POrw.Shipd2);													ordUnOk = POrw.Quant - POrw.Shipd2;												end;																											if (ordOk>0) then begin													PO2rw.Comment = PO2rw.Comment & POr.VECode &"," &  ordOk &"," ;																					end;												if (ordUnOk>0) then begin													PO2rw.Spec = PO2rw.Spec & POr.VECode & "," & ordUnOk &"," ;												end;																																				MatRowPut(PO2r,j,PO2rw);												existsItem = 1;																				end;									end;								end;								if (existsItem==0) then begin									clearrow(PO2r,PO2rw,1);																			PO2rw.ArtCode = POrw.ArtCode;										if(POr.OKFlag==1)then begin											PO2rw.Quant = POrw.Quant;											PO2rw.Shipd1 = POrw.Quant - POrw.Shipd2;											ordOk = PO2rw.Shipd1;										end else begin											PO2rw.Price = POrw.Quant;											PO2rw.Shipd2 = POrw.Quant - POrw.Shipd2;											ordUnOk = PO2rw.Shipd2;										end;																					if (ordOk>0) then begin													PO2rw.Comment = PO2rw.Comment & POr.VECode &"," &  ordOk &"," ;																					end;												if (ordUnOk>0) then begin													PO2rw.Spec = PO2rw.Spec & POr.VECode & "," & ordUnOk &"," ;												end;																MatRowPut(PO2r,k,PO2rw);// Edit ************************** Monday, 4 August 2014 17:22:52									k=k+1;// Edit ************************** Monday, 4 August 2014 17:22:51								end;																				ordOk = 0;							ordUnOk = 0;						end;												end;				end;		end;	end;	return;end;global procedure GetISStatus(string artcode,var val OrdQty,var val POQty,var val POUnOkQty,string location,var val OrdQtyRR,var val OrdQty1K)begin	record ItemStatusVc ISr;		OrdQty = 0;	POQty = 0;	POUnOkQty = 0;	OrdQtyRR = 0;	OrdQty1K = 0;		if(nonblank(location))then begin		ISr.Location = location;	end else begin		ISr.Location = ";;;";	end;	ISr.Code = artcode;	if(readfirstmain(ISr,2,true))then begin		//OrdQty = ISr.OrddOut;		POQty = ISr.POQty;		POUnOkQty = ISr.POUnOKQty;	end;		ISr.Location = "Y_БРАК";	ISr.Code = artcode;	if(readfirstmain(ISr,2,true))then begin		OrdQty = ISr.Instock;	end;		ISr.Location = "RYBRAY";	ISr.Code = artcode;	if(readfirstmain(ISr,2,true))then begin		OrdQtyRR = ISr.OrddOut;	end;	ISr.Location = "1-KOC";	ISr.Code = artcode;	if(readfirstmain(ISr,2,true))then begin		OrdQty1K = ISr.OrddOut;	end;	return;end;global procedure DblOpenRepRn(string dblstr,string l,Integer currepwn)begin	string 200 artcode;	record RcVc RepSpec;	RepSpec.f1 = l;	RepSpec.Media = mtScreen;	RepSpec.repname = "ItemPUOrderReportRn";	runreport(RepSpec,0);		endjob;return;end;global procedure DblOpenINrsRn(string dblstr,string l,Integer currepwn)begin	string 200 artcode;	record RcVc RepSpec;	ReportDefaults(RepSpec,"INrsRClass");	RepSpec.f1 = dblstr;	RepSpec.f6 = "1-KOC";	RepSpec.flags[11] = 1;	RepSpec.flags[2] = 1;	RepSpec.flags[3] = 1;	RepSpec.flags[1] = 1;	RepSpec.flags[18] = 1;	RepSpec.flags[5] = 1;	RepSpec.flags[20] = 1;	RepSpec.flags[8] = 1;	RepSpec.flags[19] = 1;	RepSpec.flags[6] = 1;	RepSpec.flags[4] = 1;	RepSpec.flags[22] = 1;	RepSpec.flags[17] = 1;	RepSpec.flags[7] = 1;	RepSpec.flags[23] = 1;	RepSpec.flags[9] = 1;	RepSpec.repname = "INrsRn";	runreport(RepSpec,0);	return;end;