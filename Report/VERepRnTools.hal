external procedure ExtractObj(string,var Integer,var string);external function Boolean SetInSet2(string,string);	SetLangMode(LangRussian,"RUS",0);globalprocedure LookupInHist(string Item, Date StartDate, Date EndDate, string Loc,var val sold,var val strRest,var val gotval,var val curRest,var val TRWH,                        var val retPU,var val SDqty,var val stin,var val stout,var val saleopt,var val saleroz,var vector val LocBalValues,var vector val PosBalValues,var val ProdIn, var val ProdOut) BEGIN   	record ItemHistVc IHr;  	Boolean TrHs,testf;  	string 50 keyloop;  	integer intloop;  	vector string 20 LocationOfPosition;  	record POSVc POSr;  	  			While(LoopMain(POSr,1,true)) begin			LocationOfPosition[POSr.Code] = POSr.Location;		end;    	  	  	  	sold = 0; // Продано  	strRest = 0;  // Начальных остаток  	gotval = 0; // Получено  	curRest = 0;  // Остаток на период  	TRWH = 0;//Transit  	saleopt = 0;  	saleroz = 0;  	SDqty = 0;  	retPU = 0;	  	TrHs = true;  	IHr.ArtCode = Item;  	keyloop = "ArtCodeLoc";  	intloop = 3;  	ProdIn = blankval;  	ProdOut = blankval;  	  	//if(blank(Loc))then begin			keyloop = "ArtCode";  		intloop = 2;  	//end;  	IHr.Location = Loc;  	//IHr.TransDate = StartDate;	//Edit----------------------Dima  17.03.2015		while (LoopKey(keyloop,IHr,intloop,TrHs)) begin			testf = true;			if (IHr.ArtCode!=Item) then begin TrHs = false; testf = false; end;			//if ((nonblank(Loc) and IHr.Location != Loc)) then begin TrHs = false; testf = false; end;			if (IHr.TransDate > EndDate) then begin TrHs = false; testf = false; end;			//if (IHr.Invalid!=0) then begin testf = false; end;						if (testf) then begin				if (IHr.TransDate < StartDate) then begin					if (IHr.StockAffectf!=0) then begin						//if(IHr.Location!="TRWH")then begin						//if(IHr.Location==Loc or blank(Loc))then begin							if(IHr.Location!="Y_БРАК" and IHr.Location!="Y_НЕТ_СКЛ" and IHr.Location!="Y_ТЕСТ") then begin	//Edit----------------------Dima  18.01.2016								strRest = strRest + IHr.Qty;								curRest = curRest + IHr.Qty;							end else begin								TRWH = TRWH + IHr.Qty;							end;						//end;						LocBalValues[IHr.Location] = LocBalValues[IHr.Location] + IHr.Qty;//Edit----------------------Dima  18.01.2016						if(nonblank(IHr.Position))then begin							if (LocationOfPosition[IHr.Position]==IHr.Location) then begin								PosBalValues[IHr.Position] = PosBalValues[IHr.Position] + IHr.Qty;							end;						end;					end;				end;				if (IHr.TransDate >= StartDate and IHr.TransDate <= EndDate) then begin					if (IHr.StockAffectf!=0) then begin						//if (IHr.Location!="TRWH") then begin						if (true) then begin								if(IHr.Location!="Y_БРАК" and IHr.Location!="Y_НЕТ_СКЛ" and IHr.Location!="Y_ТЕСТ") then begin	//Edit----------------------Dima  18.01.2016									curRest = curRest + IHr.Qty;								end;								if (IHr.FileName == "PUVc") then begin									gotval = gotval + IHr.Qty;																	end;								if (IHr.FileName == "RetPUVc") then begin									retPU = retPU + IHr.Qty;								end;								if (IHr.FileName == "SDVc") then begin									SDqty = SDqty + IHr.Qty;								end;																if (IHr.FileName == "ProdVc") then begin									if(IHr.Qty<0)then begin										ProdIn = ProdIn - IHr.Qty;									end;									if(IHr.Qty>0)then begin										ProdOut = ProdOut + IHr.Qty;									end;								end;																if (IHr.FileName == "IVVc" or IHr.FileName == "SHVc" or IHr.FileName == "RetVc") then begin									if(IHr.Location==Loc or blank(Loc))then begin											sold = sold + IHr.Qty;									end;									if(IHr.Location=="1-KOC")then begin		//Edit------------------------Dima  16:39 30.01.2015										saleopt = saleopt + IHr.Qty;									end else begin										saleroz = saleroz + IHr.Qty;									end;								end;								LocBalValues[IHr.Location] = LocBalValues[IHr.Location] + IHr.Qty;//Edit----------------------Dima  18.01.2016								if(nonblank(IHr.Position))then begin									if (LocationOfPosition[IHr.Position]==IHr.Location) then begin										PosBalValues[IHr.Position] = PosBalValues[IHr.Position] + IHr.Qty;									end;								end;																								if(IHr.Location==Loc or blank(Loc))then begin										if(nonblank(Loc))then begin										if (IHr.FileName == "StockMovVc") then begin											if(IHr.Qty>0)then begin												stin = stin + IHr.Qty;											end else begin												stout = stout + IHr.Qty;											end;										end;									end;								end;						end else begin							TRWH = TRWH + IHr.Qty;						end;					end;				end;			end;				end;	return;end;// Загон всех строк по ORVc за искомый период у виртуальных ORVcglobalprocedure CollectOrderRows(var record ORVc OR2r,Date sStartDate,Date sEndDate,string location) BEGIN   integer i,j,rwcnt,rwcnt2,existsItem,k;  Boolean TrHs,testf;  record ORVc ORr;  row ORVc ORrw,OR2rw;  	k=0;// Edit ************************** Monday, 4 August 2014 17:22:46	ORr.OrdDate = sStartDate;	TrHs = true;	while (LoopKey("OrdDate",ORr,1,TrHs)) begin	testf = true;	if (ORr.OrdDate>sEndDate) then begin TrHs = false; testf = false; end;	if (ORr.OKFlag==0) then begin testf = false; end;		if (testf) then begin	  	rwcnt = MatRowCnt(ORr);	  	if (rwcnt>0) then begin		  	for (i=0;i<rwcnt;i=i+1) begin		  	  MatRowGet(ORr,i,ORrw);		  	  if(nonblank(ORrw.ArtCode) and ORrw.stp==kInvoiceRowTypeNormal)then begin// Edit ************************** Monday, 4 August 2014 17:24:51						rwcnt2 = MatRowCnt(OR2r);							if (rwcnt2==0) then begin							if (ORr.Closed==0) then begin								OR2rw.ArtCode = ORrw.ArtCode;								OR2rw.Quant = ORrw.Quant;								OR2rw.Shipd2 = ORrw.Quant - ORrw.Shipd2;							end else begin								OR2rw.ArtCode = ORrw.ArtCode;								OR2rw.Quant = ORrw.Shipd2;								OR2rw.Shipd2 = 0;							end;								MatRowPut(OR2r,k,OR2rw);// Edit ************************** Monday, 4 August 2014 17:22:49								k=k+1;// Edit ************************** Monday, 4 August 2014 17:22:50							end else begin								existsItem = 0;								for (j=0;j<rwcnt2;j=j+1) begin									MatRowGet(OR2r,j,OR2rw);									if (OR2rw.ArtCode==ORrw.ArtCode) then begin										if (ORr.Closed==0) then begin											OR2rw.Quant = OR2rw.Quant + ORrw.Quant;											OR2rw.Shipd2 = OR2rw.Shipd2 + (ORrw.Quant - ORrw.Shipd2);										end else begin											OR2rw.Quant = OR2rw.Quant + ORrw.Shipd2;										end;										MatRowPut(OR2r,j,OR2rw);										existsItem = 1;									end;								end;								if (existsItem==0) then begin									clearrow(OR2r,OR2rw,1);									if (ORr.Closed==0) then begin										OR2rw.ArtCode = ORrw.ArtCode;										OR2rw.Quant = ORrw.Quant;										OR2rw.Shipd2 = ORrw.Quant - ORrw.Shipd2;									end else begin										OR2rw.ArtCode = ORrw.ArtCode;										OR2rw.Quant = ORrw.Shipd2;									end;									MatRowPut(OR2r,k,OR2rw);// Edit ************************** Monday, 4 August 2014 17:22:52									k=k+1;// Edit ************************** Monday, 4 August 2014 17:22:51								end;						end;		  		end;		  	end;  		end;	end;	end;	  	return;end;/*	globalprocedure CollectPORows(var record POVc PO2r,Date sStartDate,Date sEndDate,string location) BEGIN   integer i,j,rwcnt,rwcnt2,existsItem,k;  Boolean TrHs,testf;  record POVc POr;  row POVc POrw,PO2rw;    k=0;// Edit ************************** Monday, 4 August 2014 17:22:46	//POr.OKFlag = 0;	POr.ActOkFlag = 1;	TrHs = true;	//while (LoopKey("OKFlag",POr,1,TrHs)) begin	while (LoopKey("ActOkFlag",POr,1,TrHs)) begin	//Edit----------------------Dima  12.03.2015		testf = true;		if (POr.ActOkFlag==0) then begin TrHs = false; testf = false; end;		//if (nonblank(location) and SetInSet2(location,POr.Location)==false) then begin testf = false; end;				if (testf) then begin				rwcnt = MatRowCnt(POr);				if (rwcnt>0) then begin					f